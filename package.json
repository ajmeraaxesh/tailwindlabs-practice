{
    "name": "tailwindui-create-react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@fullhuman/postcss-purgecss": "^2.3.0",
        "@tailwindcss/ui": "^0.6.2",
        "@tailwindui/react": "^0.1.1",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.0.4",
        "@testing-library/user-event": "^12.1.5",
        "autoprefixer": "^9.8.4",
        "postcss-cli": "^7.1.2",
        "postcss-preset-env": "^6.7.0",
        "prettierrc": "^0.0.0-5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.3",
        "tailwindcss": "^1.8.10"
    },
    "scripts": {
        "prestart": "yarn build:styles",
        "build:styles": "postcss src/tailwind.css -o src/tailwindstyles.css",
        "start": "react-scripts start",
        "prebuild": "yarn build:stylesprod",
        "build:stylesprod": "postcss src/tailwind.css -o src/tailwindstyles.css --env production",
        "build": "react-scripts build",
        "watch": "postcss src/tailwind.css -o src/styles.css --watch",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
